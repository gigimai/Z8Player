#import "ZPMovie.h"

@implementation ZPMovie
@synthesize title;
@synthesize link;
@synthesize posterImage;

-(id)initWithTitle:(NSString *)aTitle urlLink:(NSURL *)aLink posterImage:(UIImage *)aPosterImage
{
    self = [super init];
    if (self) {
        
    
    title = aTitle;
    link = aLink;
    posterImage = aPosterImage;
    }
    return self;
}
@end
